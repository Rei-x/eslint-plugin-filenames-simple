{
  "name": "eslint-plugin-filenames-simple",
  "version": "0.1.1",
  "description": "An ESLint plugin to check filenames with simple configuration",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "filenames"
  ],
  "homepage": "https://github.com/epaew/eslint-plugin-filenames-simple",
  "repository": "git@github.com:epaew/eslint-plugin-filenames-simple",
  "license": "MIT",
  "author": "epaew <epaew.333@gmail.com>",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*",
    "eslint": "eslint --ext .js,.ts src",
    "eslint:fix": "eslint --fix --ext .js,.ts src",
    "prepack": "npm-run-all eslint test clean build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-filenames-simple": "file:.",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "eslint": "^7.0.0"
  }
}
